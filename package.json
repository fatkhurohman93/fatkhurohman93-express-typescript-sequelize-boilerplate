{
  "name": "express-typescript-sequelize-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "akbarpathur",
  "scripts": {
    "start": "rimraf dist && yarn start:build_nodemon",
    "start:build_nodemon": "concurrently --names \"build,nodemon\" \"yarn build --watch\" \"yarn nodemon\"",
    "build": "babel ./src --out-dir dist --extensions \".ts,.tsx,.js,.jsx\"",
    "nodemon": "wait-on dist/index.js && nodemon dist/index.js",
    "tsc": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "lint": "tslint -c tslint.json --fix --format verbose  src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@types/sequelize": "^4.28.11",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "sequelize": "^7.0.0-alpha.2",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^10.12.2",
    "babel-plugin-root-import": "^6.6.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
    "rimraf": "^2.6.2",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "0.0.9",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^4.6.3",
    "wait-on": "^3.2.0"
  }
}
